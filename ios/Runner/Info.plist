		<key>CFBundleURLTypes</key>
		<array>
			<dict>
				<key>CFBundleURLName</key>
				<string></string>
				<key>CFBundleURLSchemes</key>
				<array>
					<string>http</string>
					<string>https</string>
				</array>
				<key>CFBundleURLTypes</key>
				<array>
					<dict>
						<key>CFBundleURLName</key>
						<string></string>
						<key>CFBundleURLSchemes</key>
